#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds Xenome to your environment variables,"
}

module-whatis "adds Xenome to your environment variables."

set              version           1.0.0
set              root              /cm/shared/apps/xenome/$version

prepend-path     PATH              $root
prepend-path	 PATH		   /cm/local/apps/gcc/5.2.0/bin
prepend-path	 LD_LIBRARY_PATH   /cm/local/apps/gcc/5.2.0/lib:/cm/local/apps/gcc/5.2.0/lib64

