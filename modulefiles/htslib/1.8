#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds HTSlib to your environment variables,"
}

module-whatis "adds HTSlib to your environment variables."

set              version           1.8
set              root              /cm/shared/apps/htslib/$version

prepend-path      PATH              $root/bin
prepend-path      MANPATH           $root/share/man
prepend-path      LD_LIBRARY_PATH   $root/lib
