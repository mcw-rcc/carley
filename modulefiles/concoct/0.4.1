#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds CONCOCT to your environment variables,"
}

module-whatis "adds CONCOCT to your environment variables."

set              version           0.4.1
set              root              /cm/shared/apps/concoct/$version

prepend-path      PATH              $root/bin
prepend-path      LD_LIBRARY_PATH   $root/lib
prepend-path   PYTHONPATH     $root/lib/python2.7/site-packages
