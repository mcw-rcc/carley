#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds VarDict to your environment variables,"
}

module-whatis "adds VarDict to your environment variables."

set              version           09232016
set              root              /cm/shared/apps/vardict/$version

prepend-path      PATH              $root/bin
prepend-path      LD_LIBRARY_PATH   $root/lib

prepend-path      PATH              /cm/shared/apps/R/3.3.1/bin
prepend-path      MANPATH           /cm/shared/apps/R/3.3.1/share/man
prepend-path      LD_LIBRARY_PATH   /cm/shared/apps/R/3.3.1/lib
prepend-path      LD_LIBRARY_PATH   /cm/shared/apps/R/3.3.1/lib64
