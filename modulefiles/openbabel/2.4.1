#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds Open Babel to your environment variables,"
}

module-whatis "adds Open Babel to your environment variables."

set              version           2.4.1
set              root              /cm/shared/apps/openbabel/$version

prepend-path      PATH              $root/bin
prepend-path      MANPATH           $root/share/man
prepend-path      LD_LIBRARY_PATH   $root/lib:$root/lib64

