#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds KneadData to your environment variables,"
}

module-whatis "adds KneadData to your environment variables."

set              version           0.5.4
set              root              /cm/shared/apps/kneaddata/$version

prepend-path      PATH              $root/bin
prepend-path      LD_LIBRARY_PATH   $root/lib
prepend-path	  PYTHONPATH	    $root/lib/python2.7/site-packages
