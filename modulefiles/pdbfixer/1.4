#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds PDBFixer to your environment variables,"
}

module-whatis "adds PDBFixer to your environment variables."

set              version           1.4
set              root              /cm/shared/apps/pdbfixer/$version

prepend-path	 PATH		   $root/bin
prepend-path     LD_LIBRARY_PATH   $root/lib
prepend-path     PYTHONPATH        $root/lib/python3.6/site-packages
